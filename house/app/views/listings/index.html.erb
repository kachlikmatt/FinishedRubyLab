<h1>All Listings:</h1>

<table>
  <tr>
    <th>Address:</th>
    <th>Price:</th>
    <th>Description:</th>
  </tr>

  <% @listings.each do |listing| %>
    <tr>
      <td><%= listing.address %></td>
      <td><%= listing.price %></td>
      <td><%= listing.description %></td>
      <td><%= link to 'Show',listing_path(listing) %> </td>
      <td><%= link to 'Edit',edit_listing_path(listing) %> </td>
    </tr>
  <% end %>
</table>
